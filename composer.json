{
  "name": "becklyn/eventor-symfony",
  "description": "A minimalistic library for abstracting pub/sub operations (ported for Symfony)",
  "type": "symfony-bundle",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Samuel Oechsler",
      "email": "so@becklyn.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "cloudevents/sdk-php": "^1.0",
    "illuminate/collections": "^8.12 | ^9.25",
    "symfony/http-client": "6.1.*",
    "symfony/http-foundation": "6.1.*",
    "symfony/http-kernel": "6.1.*",
    "symfony/serializer": "6.1.*",
    "symfony/dependency-injection": "6.1.*",
    "symfony/config": "6.1.*",
    "symfony/framework-bundle": "^6.1",
    "guzzlehttp/psr7": "^2.4",
    "psr/http-client": "^1.0",
    "open-telemetry/opentelemetry": "^0.0.15"
  },
  "autoload": {
    "psr-4": {
      "Becklyn\\Eventor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Becklyn\\Eventor\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
